stration/src/components/index.js",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/Registration.jsx",["144","145"],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/Header.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/Login.jsx",["146"],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/ResetPassword.jsx",["147"],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/RestorePassword.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/Congratulations.jsx",["148"],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/PasswordsSet.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/TextInput.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/EmailInput.jsx",["149","150","151","152"],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/Image.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/PasswordRequirements.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/SinglePasswordRequirement.jsx",[],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/EmailConfirmation.jsx",["153"],"/Users/arsenypomazkov/Desktop/projects/api_registration/src/components/ResetPasswordConfirmation.jsx",["154"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/index.js",[],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/App.jsx",[],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/index.js",[],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/RestorePassword.jsx",[],["155","156"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/Registration.jsx",["157","158","159"],"import React, { useState, useEffect } from \"react\";\n\nimport PasswordsSet from \"./PasswordsSet\";\nimport PasswordRequirements from \"./PasswordRequirements\";\nimport EmailInput from \"./EmailInput\";\nimport TextInput from \"./TextInput\";\nimport pdnDocument from \"../assets/documents/obrabotkа_pdn_banka_vtb_pao.pdf\";\nimport agreementDocument from \"../assets/documents/Оферта.pdf\";\n\n\nfunction Registration() {\n    // Объект с пользовательскими данными\n    const [userInfo, setUserInfo] = useState({\n        name: \"\",\n        surname: \"\",\n        username: \"\",\n        organization: \"\",\n        password: \"\",\n        repeatPassword: \"\",\n        termsOfUse: false,\n        organizationRegulations: false\n    });\n\n    const [isButtonEnabled, setButtonEnabled] = useState(false)\n\n    useEffect(() => {\n        for (let item in userInfo) {\n            if (!Boolean(userInfo[item])) {\n                setButtonEnabled(false)\n                return\n            }\n        }\n        setButtonEnabled(true)\n    })\n\n    // Результаты валидации, происходящей в компоненте PasswordsSet\n    const [isSymbolTypes, setSymbolTypes] = useState(null);\n    const [isLengthCorrect, setLengthCorrect] = useState(null);\n    const [isPasswordContainUsername, setPasswordContainUsername] = useState(null);\n    const [isSameSymbolsSequence, setSameSymbolsSequence] = useState(null);\n\n    function handleUserInput(event) {\n        const {value, name} = event.target;\n        setUserInfo((prevValue) => {\n            return {\n                ...prevValue,\n                [name]: value\n            }\n        });\n    }\n\n    function handlePasswordInput(data) {\n        setUserInfo((prevValue) => {\n            return {\n                ...prevValue,\n                password: data.password,\n                repeatPassword: data.repeatPassword\n            }\n        });\n    }\n\n    function handleCheckboxInput(event) {\n        const {name} = event.target;\n        setUserInfo((prevValue) => {\n            return {\n                ...prevValue,\n                [name]: !userInfo[name]\n            }\n        });\n    }\n\n    return <main className=\"body\">\n        <div className=\"registration-content\">\n            <p className=\"page-content-p\">Портал разработчика API</p>\n            <h1 className=\"page-content-h1\">Регистрация</h1>\n            <div className=\"registration-input-wrapper\" >\n                <label className=\"input-label\" htmlFor=\"username\">Адрес электронной почты (логин)</label>\n                <EmailInput \n                    emailHandle={handleUserInput}\n                    currentValue={userInfo.username}\n                    autofocus\n                />\n            </div>\n            <div className=\"registration-input-wrapper\" >\n                <label className=\"input-label\" htmlFor=\"name\">Имя</label>\n                <TextInput\n                    name=\"name\"\n                    placeholder=\"Введите имя\"\n                    currentValue={userInfo.name}\n                    handleInput={handleUserInput}\n                />\n            </div>\n            <div className=\"registration-input-wrapper\" >\n                <label className=\"input-label\" htmlFor=\"surname\">Фамилия</label>\n                <TextInput\n                    name=\"surname\"\n                    placeholder=\"Введите фамилию\"\n                    currentValue={userInfo.surname}\n                    handleInput={handleUserInput}\n                />\n            </div>\n            <div className=\"registration-input-wrapper\" >\n                <label className=\"input-label\" htmlFor=\"organization\">Название организации</label>\n                <TextInput\n                    name=\"organization\"\n                    placeholder=\"Введите название организации\"\n                    currentValue={userInfo.organization}\n                    handleInput={handleUserInput}\n                />\n            </div>\n\n            <PasswordsSet \n                username={userInfo.username}\n                savePassword={handlePasswordInput}\n                setSymbolTypes={setSymbolTypes}\n                setLengthCorrect={setLengthCorrect}\n                setPasswordContainUsername={setPasswordContainUsername}\n                setSameSymbolsSequence={setSameSymbolsSequence} \n            />\n\n            <div className=\"conditions_accept-wrapper conditions_accept-wrapper-first\">\n                <input\n                    className=\"conditions_accept-wrapper-custom_checkbox\"\n                    type=\"checkbox\"\n                    id=\"termsOfUse\"\n                    name=\"termsOfUse\"\n                    onChange={handleCheckboxInput}\n                />\n                {/* Необходимо добавить аттрибут href ссылке ниже */}\n                <label className=\"conditions_accept-wrapper-label\" htmlFor=\"termsOfUse\"><p className=\"conditions_accept-wrapper-p\">Принимаю <a className=\"\" href={agreementDocument} target=\"_blank\">пользовательское соглашение</a></p></label>\n            </div>\n            <div className=\"conditions_accept-wrapper conditions_accept-wrapper-second\">\n                <input\n                    className=\"conditions_accept-wrapper-custom_checkbox\"\n                    type=\"checkbox\"\n                    id=\"organizationRegulations\"\n                    name=\"organizationRegulations\"\n                    onChange={handleCheckboxInput}\n                />\n                <label className=\"conditions_accept-wrapper-label\" htmlFor=\"organizationRegulations\"><p className=\"conditions_accept-wrapper-p\">Принимаю <a className=\"\" href={pdnDocument} target=\"_blank\">правила Положения об организации обработки персональных данных в Банке ВТБ (ПАО)</a></p></label>\n            </div>\n\n            <div className=\"page-content-actions\">\n                <button disabled={!isButtonEnabled} className=\"page-content-button page-content-button-blue registration-button-blue\" type=\"submit\">Регистрация</button>\n                <div className=\"page-content-actions-login\">\n                    <p>Уже есть аккаунт?</p>\n                    <a className=\"page-content-link\" href=\"/\">Войти</a>\n                </div>\n            </div>\n        </div>\n        <PasswordRequirements\n            symbolsCheck={isSymbolTypes}\n            lengthCheck={isLengthCorrect}\n            usernameInPasswordCheck={isPasswordContainUsername}\n            sameSymbolsCheck={isSameSymbolsSequence}  \n        />\n    </main>\n}\n\nexport default Registration;","/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/Login.jsx",["160"],"import React, {useState} from \"react\";\n\nimport loginImg from \"../assets/images/login_API-07.png\";\nimport Image from \"./Image\";\nimport EmailInput from \"./EmailInput\";\nimport closedEyeImg from \"../assets/images/closed_eye.svg\";\nimport openedEyeImg from \"../assets/images/opened_eye.svg\";\n\n\nfunction Login() {\n    // Объект с пользовательскими данными\n    const [inputInfo, setInputInfo] = useState({\n        username: \"\",\n        password: \"\"\n    });\n\n    const closed_eye = <img className=\"input-icon-eye_img\" src={closedEyeImg} alt=\"eye\"></img>;\n    const opened_eye = <img className=\"input-icon-eye_img\" src={openedEyeImg} alt=\"eye\"></img>;\n    const [passwordShown, setPasswordShown] = useState(false);\n    function togglePasswordVisiblity() {\n      setPasswordShown(!passwordShown);\n    };\n    \n    function handleInput(event) {\n        const {name, value} = event.target;\n        setInputInfo((prevValue) => {\n            return {\n                ...prevValue,\n                [name]: value\n            }\n        })\n    }\n\n    return <main className=\"body\">\n        <Image img={loginImg} style={\"decorative_image\"}/>\n        <div className=\"page-content\">\n            <p className=\"page-content-p\">Портал разработчика API</p>\n            <h1 className=\"page-content-h1\">Войти в аккаунт</h1>\n            <EmailInput \n                emailHandle={handleInput}\n                currentValue={inputInfo.username}\n            />\n            <div className=\"login-password-wrapper\">\n                <input\n                    name=\"password\"\n                    className=\"page-content-input\"\n                    placeholder=\"Введите пароль\"\n                    type={passwordShown ? \"text\" : \"password\"}\n                    value={inputInfo.password}\n                    onChange={handleInput}\n                />\n                <i className=\"input-icon\" onClick={togglePasswordVisiblity}>{passwordShown ? closed_eye : opened_eye}</i>\n            </div>\n            <div className=\"page-content-actions\">\n                <button className=\"page-content-button page-content-button-blue login-button-blue\" type=\"submit\">Войти</button>\n                <a href=\"/password-reset\"><button className=\"page-content-button page-content-button-transparent\">Восстановить пароль</button></a>\n            </div>\n            <a className=\"page-content-link\" href=\"/registration\">Создать аккаунт</a>\n        </div>\n    </main>\n}\n\nexport default Login;","/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/Congratulations.jsx",["161"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/ResetPassword.jsx",["162"],"import React, {useState} from \"react\";\n\nimport resetPassImg from \"../assets/images/login_API-08.png\";\n\nimport Image from \"./Image\";\nimport EmailInput from \"./EmailInput\";\n\n\nfunction ResetPassword() {\n    const [email, setEmail] = useState(\"\");\n\n    function handleInput(event) {\n        const {value} = event.target;\n        setEmail(value);\n    }\n\n    return <main className=\"body\">\n        <Image img={resetPassImg} style={\"decorative_image\"}/>\n        <div className=\"page-content\">\n            <p className=\"page-content-p\">Портал разработчика API</p>\n            <h1 className=\"page-content-h1\">Сброс пароля</h1>\n            <p className=\"page-content-p page-content-description\">Если вы забыли пароль от своей учётной записи, отправьте запрос на сброс пароля</p>\n            <EmailInput \n                emailHandle={handleInput}\n                currentValue={email}\n            />\n            <p className=\"page-content-p page-content-muted reset_pass-instructions\">Инструкции по сбросу пароля будут отправлены на ваш зарегистрированный адрес электронной почты</p>\n            <div className=\"page-content-actions\">\n                <button className=\"page-content-button page-content-button-blue reset_pass-button-blue\" type=\"submit\">Отправить</button>\n                <a href=\"/\" className=\"page-content-button page-content-button-transparent\" type=\"submit\">Отмена</a>\n                {/* <a className=\"page-content-link\" href=\"/\">Вернуться к форме входа в систему</a> */}\n            </div>\n        </div>\n    </main>\n}\n\nexport default ResetPassword;","/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/Header.jsx",[],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/EmailConfirmation.jsx",["163"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/ResetPasswordConfirmation.jsx",["164"],"import React, {useState} from \"react\";\nimport TextInput from \"./TextInput\";\n\nimport confirmationImg from \"../assets/images/login_API-09.png\";\n\nimport Image from \"./Image\";\n\n\n\nfunction Confirmation() {\n    // Объект с пользовательскими данными\n    const username = \"Konstantin.Sorokin.1981@yandex.ru\";\n    const [confirmationCode, setConfirmationCode] = useState(\"\");\n\n    function handleCodeInput(event) {\n        const {value} = event.target;\n        setConfirmationCode(value);\n    }\n\n    return <main className=\"body\">\n        \n        <Image img={confirmationImg} style={\"decorative_image\"}/>\n        <div className=\"confirmation-content\">\n            <p className=\"page-content-p\">Портал разработчика API</p>\n            <h1 className=\"page-content-h1\">Подтверждение сброса пароля</h1>\n            <p className=\"confirmation-content-p\">Мы отправили Вам код подтверждения на почту</p>\n            <p className=\"confirmation-content-username\">{username},</p>\n            <p className=\"confirmation-content-p\">введите его в поле ниже для сброса пароля</p>\n            <div className=\"confirmation-content-input-wrapper\" >\n                <label className=\"input-label\" htmlFor=\"code\">Код, отправленный на email</label>\n                <TextInput\n                    name=\"code\"\n                    placeholder=\"Введите код подтверждения\"\n                    currentValue={confirmationCode}\n                    handleInput={handleCodeInput}\n                />\n            </div>\n            <div className=\"page-content-actions\">\n                <button className=\"page-content-button page-content-button-blue confirmation-content-button-blue\" type=\"submit\">Подтвердить</button>\n                <a className=\"page-content-link\" href=\"/\">Отменить и вернуться к форме входа в систему</a>\n            </div>\n        </div>\n    </main>\n}\n\nexport default Confirmation;","/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/PasswordRequirements.jsx",["165"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/PasswordsSet.jsx",["166","167","168","169"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/TextInput.jsx",[],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/EmailInput.jsx",["170","171","172","173"],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/Image.jsx",[],"/Users/arsenypomazkov/Desktop/Projects/api_registration/src/components/SinglePasswordRequirement.jsx",[],{"ruleId":"174","severity":1,"message":"175","line":115,"column":141,"nodeType":"176","endLine":11